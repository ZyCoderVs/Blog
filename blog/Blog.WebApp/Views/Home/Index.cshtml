
@{
    Layout = null;
}
@using Blog.Entity
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div>
        <li>
            @if (ViewBag.Users != null)
            {
                foreach (User item in ViewBag.Users)
                {
                    <ul>@item.UserName</ul>
                }
            }
        </li>
    </div>
</body>
</html>
